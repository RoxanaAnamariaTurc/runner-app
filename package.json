{
  "name": "runhope",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web",
    "preview": "npx serve dist",
    "optimize-images": "node scripts/optimize-images.js",
    "build:optimized": "npm run optimize-images && npm run build:web",
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "analyze-deps": "node scripts/analyze-dependencies.js",
    "type-check": "tsc --noEmit",
    "lint": "npx expo lint",
    "bundle-analyze": "npm run build:web && npx webpack-bundle-analyzer dist/static/js/*.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "expo": "~53.0.17",
    "expo-constants": "~17.1.7",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-status-bar": "~2.2.3",
    "i18next": "^25.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.6.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "sharp": "^0.34.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
